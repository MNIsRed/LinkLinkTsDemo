/**index.wxss**/
.gradientBackground{
  position: absolute;
  top: 0;
  width: 100%;
  height: 500rpx;
  background: linear-gradient(#CFFFF1,#CFFFF100);
}

#tryAgain{
  width: 206rpx;
  height: 206rpx;
  z-index: 1009;
  position: absolute;
  top: 50%;
  right: 50%;
  transform: translate(-30rpx, -30rpx);
}

#next{
  width: 206rpx;
  height: 206rpx;
  z-index: 1009;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(30rpx, -30rpx);
}

#bottomTip{
  z-index: 1009;
  position: absolute;
  top: 50%;
  left: 50%;
  width: max-content;
  display: flex;
  flex-direction: row;
  transform: translate(-50%, 220rpx);
  align-items: center;
}

#linkItem{
  margin-bottom: 200rpx;
}

.center-message-bg {
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 48rpx;
  margin-right: 48rpx;
}

.center-notify-bg {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: relative;
  width: 144rpx;
  height: 62rpx;
  background-color: #DCFCDE;
  align-items: center;
  border-radius: 31rpx;
}

.center-notify-text {
  font-size: 24rpx;
  font-weight: 350;
  color: #0A7810;
  position: absolute;
  left: 68rpx;
}